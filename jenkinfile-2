pipeline {
    agent any
    stages{

        stage('Build'){

            steps{
                sh 'mvn clean package'
            }
        }
        stage('Archive Artifacts'){

            steps{
                echo "Archiving Arctifacts..."
                archiveArtifacts arctifacts: '**/*.war'
                
            }
        }
        stage('Deploy to stage'){

            steps{
                build job: 'deploy-stagging'
                
            }
        }
    }
}
